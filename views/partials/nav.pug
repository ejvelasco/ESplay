#nav
	nav.navbar.navbar-default
		.container-fluid
			.navbar-header
				button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#bs-example-navbar-collapse-1', aria-expanded='false')
					span.sr-only Toggle navigation
					span.icon-bar
					span.icon-bar
					span.icon-bar
				a.navbar-brand(href='') #[span.bold ES] #[span.blue play]
			#bs-example-navbar-collapse-1.collapse.navbar-collapse
				ul.nav.navbar-nav
					li(class="dropdown")
						a(href="" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false") #[span.caret]  Examples
						ul(class="dropdown-menu")
							li(ng-repeat="example in examplesES6")
								a(href="" ng-click="selectExample(example)") {{ example.title }}
					li
						a(ng-click="transpile()") #[span.glyphicon.glyphicon-play] Run
					li
						a(ng-click="clear()") #[span.glyphicon.glyphicon-erase] Clear
					li(class="dropdown")
						a(href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false") #[span.glyphicon.glyphicon-cog] Preferences
						ul(class="dropdown-menu")
							li(class="dropdown-submenu")
								a(class="test newDropDown" tabindex="-1" href="#") Load library #[span(class="caret")]
									ul(class="dropdown-menu")
										li(ng-repeat="lib in libs")
											a( class = "dd-link lib-{{$index}}" tabindex="-1" href="" ng-click="selectLib(lib)") {{ lib.name }}
							li(role="separator" class="divider")
							li(class="dropdown-submenu")
								a(class="test newDropDown" tabindex="-1" href="#") Choose theme #[span(class="caret")]
									ul(class="dropdown-menu")
										li(ng-repeat="theme in themes")
											a(class = "dd-link" tabindex="-1" href="" ng-click="selectTheme(theme)") {{ theme.name }}
	
										